{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "display": {
      "$id": "/properties/display",
      "type": "array",
      "minItems": 1,
      "items": {
        "$id": "/properties/display/items",
        "type": "string",
        "title": "Feature ListItem"
      }
    },
    "features": {
      "$id": "/properties/features",
      "type": "object",
      "properties": {
        "app_limit": {
          "$id": "/properties/features/properties/app_limit",
          "type": ["number", "null"],
          "title": "App Limit"
        },
        "branding": {
          "$id": "/properties/features/properties/branding",
          "type": "boolean",
          "title": "Show Branding?"
        },
        "theming": {
          "$id": "/properties/features/properties/theming",
          "type": "boolean",
          "title": "Allow Theming?"
        }
      },
      "required": [
        "app_limit",
        "branding"
      ]
    },
    "badge": {
      "$id": "/properties/badge",
      "type": "string",
      "title": "Badge Text"
    }
  },
  "required": [
    "display",
    "features"
  ]
}