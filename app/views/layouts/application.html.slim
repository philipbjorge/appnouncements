doctype html
html
  head
    title
      | Appnouncements
    = csrf_meta_tags
    = csp_meta_tag
    = stylesheet_link_tag    'application', media: 'all'
    = javascript_include_tag 'application'
    = render partial: "analytics"
  body
    = render :partial => 'navbar'
    = render :partial => 'toasts'
    .mt-3
    == content_for?(:content) ? yield(:content) : yield
    = any_login_here if defined?(AnyLogin)
    .pt-3
    footer.footer
      .container
        span.text-muted
          | Â© 2018 Appnouncements
        span.text-muted.float-right
          a href="https://www.appnouncements.com/terms-of-service/" Terms of Service
          |  | 
          a href="https://www.iubenda.com/privacy-policy/83452620" Privacy Policy
